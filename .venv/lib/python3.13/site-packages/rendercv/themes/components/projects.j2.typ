((* for project in entry.projects *))
  #v(design-projects-top-margin); #h(design-projects-indent); #strong[<<project.name>>]
  ((* if project.highlights *))
    ((* for item in project.highlights *))
        ((* if loop.first *))
            #v(design-highlights-top-margin);#highlights(
        ((*- endif -*))
            [<<item>>], 
        ((*- if loop.last -*))
            )
        ((* endif *))
    ((* endfor *))
  ((* endif *))
((* endfor *))